                var rgScreenshotURLs = {"ss_487947e6eb5e8ca8f71e8bfea77753c95eca1b25.jpg":"https:\/\/cdn.akamai.steamstatic.com\/steam\/apps\/1506830\/ss_487947e6eb5e8ca8f71e8bfea77753c95eca1b25_SIZE_.jpg?t=1644868577","ss_f526296665433ef5475972b690d51f9b6eebf991.jpg":"https:\/\/cdn.akamai.steamstatic.com\/steam\/apps\/1506830\/ss_f526296665433ef5475972b690d51f9b6eebf991_SIZE_.jpg?t=1644868577","ss_2cdf78a2091db6adb7ec405f4c5438fd621266a9.jpg":"https:\/\/cdn.akamai.steamstatic.com\/steam\/apps\/1506830\/ss_2cdf78a2091db6adb7ec405f4c5438fd621266a9_SIZE_.jpg?t=1644868577","ss_ab3f290ac0667490be79a1f3231cb563b1effeb4.jpg":"https:\/\/cdn.akamai.steamstatic.com\/steam\/apps\/1506830\/ss_ab3f290ac0667490be79a1f3231cb563b1effeb4_SIZE_.jpg?t=1644868577","ss_a744fe858eb67516e46bd045e165147d866a0dbd.jpg":"https:\/\/cdn.akamai.steamstatic.com\/steam\/apps\/1506830\/ss_a744fe858eb67516e46bd045e165147d866a0dbd_SIZE_.jpg?t=1644868577"};
